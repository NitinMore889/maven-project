pipeline {

      any agent {

              stages {
		      stage (' SCM cehckout') 
		      	  {
		      	  git 'https://github.com/NitinMore889/maven-project.git'
		      	  }
		     		
	      	      stage ('Install Code')
	                  {
		           steps {
		              withMaven (maven : 'LocalMaven') 
		                { sh 'mva install' }
		                }
		           }
	             }
		   }
		  }
