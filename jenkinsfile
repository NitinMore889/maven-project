pipeline {

    agent any
	
stages {

    stage ('SCM Checkout') 
	{
	  steps {
	  git 'https://github.com/NitinMore889/maven-project' 
	        }
    } 
 	stage ('Compile Stage') {
	  steps	{
	   withMaven(maven : 'LocalMaven')
	   sh 'mvn clean compile'
	   	}
	}
		stage ('Test Stage') {
	  steps	{
	   withMaven(maven : 'LocalMaven')
	   sh 'mvn test'
	        }
	}
		stage ('Package Stage') {
	  steps	{
	   withMaven(maven : 'LocalMaven')
	   sh 'mvn package'
	        }
	}
	    }
	    }
